    "version": "0.2.0",
    "configurations": [
        
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/serial.js"
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Serialport listening",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${workspaceFolder}/serial.js",
            "args": ["/dev/tty.usbserial-FTDN0YWQ", "38400"]
        }
    ]
}